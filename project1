#include <iostream>
using namespace std; 
int main(){

setlocale(LC_ALL,"Turkish"); //türkçe karakter

int i;
int a;
cout <<"bir sayi giriniz: " << endl;
cin >> a;

cout << a << " e kadar olan tamsayılar: " << endl;
if(a >= 0){

for( i=0; i<=a; i++)
{
	cout << i << endl;
	
	 
}
}
else if(a < 0){
	for(i=0; i>=a; i--)
	{
		cout << i << endl;
	}
}


}
	


